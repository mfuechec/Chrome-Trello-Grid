{
    "manifest_version": 2,
   
    "name": "Trello Grid",
    "version": "0.0.0.2",
    "description": "Adds grid layout to Trello",
    "icons": {
      "16": "grid.png",
      "48": "promotional.png",
      "128": "screenshot.png"
    },
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["https://trello.com/*"]
    }]
   
  }